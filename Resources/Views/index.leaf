#extend("base")

#export("body") {
  #loop(posts, "post") {
    <div class="row">
      <div class="six columns">
        <h5><span>#(post.title)</span></h5>
      </div>
      <div class="three columns">
        <p><span>#(post.author)</span></p>
      </div>
      <div class="three columns">
      </div>
     </div>
  }
}
