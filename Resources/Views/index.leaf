#extend("base")

#export("body") {
  #loop(posts, "post") {
    <div class="row">
      <div class="nine columns">
        <h5><span>#(post.title)</span></h5>
      </div>
      <div class="three columns">
        <form action="/posts/#(post.id)/delete" method="post">
          <input type="submit" value="Delete"/>
        </form>
      </div>
     </div>
  }
}
