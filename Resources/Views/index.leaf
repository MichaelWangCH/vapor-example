#extend("base")

#export("body") {
    #loop(posts, "post") {
        <div class="row">
            <div class="three columns">
            </div>
            <div class="three columns">
                <h5><span>#(post.title)</span></h5>
            </div>
            <div class="three columns">
                <p>#(post.author)</p>
            </div>
        </div>
    }
}
